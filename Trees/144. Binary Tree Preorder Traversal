class Solution {
    public List<Integer> preorderTraversal(TreeNode root) {
        List<Integer> list = new ArrayList<Integer>();
        preorder(root,list);
        return list;
    }
    public void preorder(TreeNode temp, List<Integer> li){
        if(temp == null)
            return ;
        li.add(temp.val);
        preorder(temp.left,li);
        preorder(temp.right,li);
    }
}

class Solution {
    public int firstUniqChar(String s) {
        Map<Character, Integer> res = new HashMap();
        for(char c:s.toCharArray()){
            if(res.containsKey(c))
                res.put(c,res.get(c)+1);
            else
                res.put(c,1);
        }
        for(int i=0;i<s.length();i++){
            if(res.get(s.charAt(i))==1)
                return i;
        }
        return -1;
    }
}

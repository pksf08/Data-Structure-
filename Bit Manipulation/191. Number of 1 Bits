class Solution {
    public int hammingWeight(int n) {
        StringBuffer s =new StringBuffer();
        while (n>0){
            s.append(n%2);
            n/=2;
        }
        int count=0;
        for(int i=0;i<s.length();i++){
            if(s.charAt(i)=='1')
                count+=1;
        }
        return count;
    }
}

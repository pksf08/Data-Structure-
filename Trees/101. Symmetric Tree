class Solution {
    public boolean isSymmetric(TreeNode root) {
        return root==null || issymmetrictrue(root.left,root.right);
    }
    public boolean issymmetrictrue(TreeNode left,TreeNode right){
        if(left==null || right==null)
            return left==right;
        if(left.val!=right.val)
            return false;
        return issymmetrictrue(left.left,right.right) && issymmetrictrue(left.right,right.left);
    }
}

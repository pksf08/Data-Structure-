class Solution {
    public boolean isValidBST(TreeNode root) {
        ArrayList<Integer> res = new ArrayList();
        inorder(root,res);
        for(int i = 1; i<res.size();i++){
            if(res.get(i)<=res.get(i-1)){
                return false;
            }
        }
        return true;
    }
    public void  inorder(TreeNode temp,List<Integer> li){
        if(temp ==null)  return  ;
        inorder (temp.left,li);
        li.add(temp.val);
        inorder (temp.right,li);
    }
}

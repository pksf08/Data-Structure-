class Solution {
    public List<Integer> inorderTraversal(TreeNode root) {
        List<Integer> li = new ArrayList<Integer>();
        inorder(root,li);
        return li;
    }
    public List<Integer> inorder(TreeNode root,List l){
         if(root == null)
            return null;
        inorder(root.left,l);
        l.add(root.val);
        inorder(root.right,l);
        return l;
    }
}

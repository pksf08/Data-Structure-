class Solution {
    public List<Integer> postorderTraversal(TreeNode root) {
        List<Integer> list =new ArrayList<>();
        postorder(root,list);
        return list;
    }
    public void postorder(TreeNode node,List<Integer> li){
        if(node == null)
            return;
        postorder(node.left,li);
        postorder(node.right,li);
        li.add(node.val);
    }
}

class Solution {
    public int maxDepth(String s) {
        int maxdepth=0,current=0;
        for(char c : s.toCharArray()){
            if(c =='('){
                current++;
                maxdepth = Math.max(maxdepth,current);
            }
            if(c==')'){
                current--;
            }
        }
        return maxdepth;
    }
}

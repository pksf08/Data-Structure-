class Solution {
    public boolean isValid(String s) {
        Stack<Character> sk =new Stack();
        for(char c : s.toCharArray()){
           if(c =='{' || c =='(' || c =='[' )
           {
               sk.push(c);
           }
           else if(c =='}' || c ==']' || c ==')'){
               
               if (sk.isEmpty())
                   return false;
               char t = sk.pop();
               if(t=='{' && c!='}' ||
                  t=='(' && c!=')' ||
                  t=='[' && c!=']' )
                   return false;  
        }
        }
         return sk.isEmpty();
    }
}

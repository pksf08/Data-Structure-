class Solution {
    public int countPrimeSetBits(int left, int right) {
        int count=0;
        for(int i =left ;i<=right;i++){
            int res = Integer.bitCount(i);
            if(isprime(res))
                count++;
        }
        return count;
    }
    public boolean isprime(int var){
        if(var<2)   return false;
        for(int i=2;i*i<=var;i++){
            if(var%i==0)
                return false;
        }        
        return true;
    }
}

class Solution {
    public int titleToNumber(String columnTitle) {
        int res =0;
        int power = 0;
        for(int i=columnTitle.length()-1;i>=0;i--){
            int val = columnTitle.charAt(i) -'A' +1;
            res +=  (val*Math.pow(26,power));
            power++;
        }
        // for(char ch:columnTitle.toCharArray()){
        //     int value = ch-'A'+1;
        //     res = res*26 + value;
        // }
        return res;
    }
}

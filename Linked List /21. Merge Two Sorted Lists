class Solution {
    public ListNode mergeTwoLists(ListNode list1, ListNode list2) {
        ListNode head = new ListNode();
        ListNode res= head;
        
        while(list1!=null && list2!=null){
            if(list1.val > list2.val){
                res.next = list2;
    
                list2 = list2.next;
            }
            else{
                res.next = list1;
                
                list1 =list1.next;
            }
            res = res.next;
        }
        res.next =(list1!= null)?list1:list2;
        return head.next;
    }
}

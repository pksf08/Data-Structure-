class Solution {
    public int mySqrt(int x) {
        int l=1;
        int r=x/2;
        int mid= 0;
        if(x<2) return x;
        while(l<=r){
            mid= l+ (r-l)/2;
            long square = (long)mid*mid;
            if(square == x)
                return mid;
            else if (square >x)
                 r= mid-1;
            else
                l=mid+1;
        }
         return r;
    }
}

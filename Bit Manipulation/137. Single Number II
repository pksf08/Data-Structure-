class Solution {
    public int singleNumber(int[] nums) {
        HashMap<Integer,Integer> hs= new HashMap<Integer,Integer>();
        for(int i:nums){
            if(hs.containsKey(i))
                hs.put(i,hs.get(i)+1);
            else
                hs.put(i,1);
        }
        for(int j : nums){
            if(hs.get(j)==1)
                return j;
        }
        return -1;
    }
}

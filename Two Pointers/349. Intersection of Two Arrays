class Solution {
    public int[] intersection(int[] nums1, int[] nums2) {
        HashSet<Integer> set = new HashSet<Integer>();
        HashSet<Integer> intersect = new HashSet<Integer>();
        int n1=nums1.length;
        int n2=nums2.length;
        for(int i:nums1){
            set.add(i);
        }
        for(int j:nums2){
            if(set.contains(j)){
                intersect.add(j);
            }
        }
        
        int k=0;
        int[] arr=new int[intersect.size()];
        for(int n:intersect){
            arr[k++] =n;
        }
        return arr;
    }
        
}
